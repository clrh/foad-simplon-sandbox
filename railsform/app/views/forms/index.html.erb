<h1>Hello</h1>
<%= form_for :form do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :comment %><br>
    <%= f.text_area :comment %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>
<h2>List</h2>
 
<% @forms.each do |r| %>
  <ul>
    <li><%= r.title %> ::</li>
  </ul>
<% end %>
